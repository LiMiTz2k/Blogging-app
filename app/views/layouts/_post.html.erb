<div class="col-md-8">
  <div class="post-section">
    <% @posts.each do |p| %>
    <div class="row">
        <div class="post">
          <div class="text-wrapper">
            <h1><%= p.title %></h1>
            <h4>By <%=user_handler(post: p)%></h4>
            <p><%= p.content %></p>
          </div>
        </div>
    </div>
    <% end %>
  </div>
</div>
